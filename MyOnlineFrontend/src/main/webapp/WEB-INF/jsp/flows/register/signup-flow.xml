<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
                          http://www.springframework.org/schema/webflow/spring-webflow.xsd">
                          
      <var name="user" class="com.niit.MyOnlineBackend.model.User" />
      <var name="billing" class="com.niit.MyOnlineBackend.model.Address" />
                          
 <!-- Flow Start State -->
	<view-state id="personal" view="flows/register/signup-personal" model="user">
	<transition on="billing" to="address" />
	</view-state>
	
	<view-state id="address" view="flows/register/signup-address" model="billing">
	<transition on="personal" to="personal" />
	<transition on="confirm" to="confirm" />
	</view-state>
	
	<view-state id="confirm" view="flows/register/signup-confirm">
	<transition on="personal" to="personal" />
	<transition on="billing" to="address" />
	<transition on="success" to="success" />
	</view-state>
	
	
	<view-state id="success" view="flows/register/signup-success" >
	</view-state>	
		

</flow>